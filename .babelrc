{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs",
      "useBuiltIns": "usage",
      "corejs": 3
    }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ]
}
